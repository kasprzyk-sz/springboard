{% include "header.njk" %}
<div class="wrapper">
  <!-- BUS -->

  {% include "first-box.njk" %}
  {% include "second-box.njk" %}
  <div class="third-container">
  <div class="vf-grid | vf-grid__col-2">
        <div class="test-class">
              <h4 class="vf-section-header__heading">EMBL News</h4>
<link rel="import" href="https://dev.content.embl.org/api/v1/pattern.html?filter-content-type=article&filter-field-value%5Bfield_article_type%5D=article_timely&filter-fields-exists=field_display_title%2Cfield_teaser%2Cfield_canonical_location&limit=2&pattern=vf-news-item-default&sort-field-value%5Bcreated%5D=DESC" data-target="self" data-embl-js-content-hub-loader>
</div>
    <div class="seminars-test">
      <div class="seminars">
        <h4 class="vf-section-header__heading">Seminars</h4>
        <div id="seminars-embed">
          {%for item in seminars.items%}
          <p class="vf-summary__date">{{item.date}}</p>
          <p class="vf-summary__date">{{item.room}}</p>
          <h6 class="vf-summary__text">{{item.title}}</h6>
          <p class="vf-summary__date">{{item.firstname}} {{item.name}}</p>
          </br>
          <hr class="vf-divider">
          {%endfor%}
        </div>
        <a href="#" class="vf-link vf-link--hover | see-more">Seminars calendar >></a>
      </div>
    </div> 
    </div>
    </div>
  <hr class="vf-divider">
  {% include "links.njk" %}
  <!-- SOCIAL MEDIA -->
  <div class="vf-grid | vf-grid__col-3 | social-media">
    <div class="sm-feeds">
      <!-- <i class="social-m fab fa-twitter-square"></i> -->
      <a class="twitter-timeline" data-width="370" data-height="400"
        href="https://twitter.com/embl?ref_src=twsrc%5Etfw">Tweets by embl</a>
      <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
    </div>
  </div>
</div>







