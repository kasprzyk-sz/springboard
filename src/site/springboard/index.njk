layout: layouts/base.njk
templateEngineOverride: njk
---
{% include "header.njk" %}
<div class="wrapper container">
  <!-- BUS & SEARCH BAR -->
  <div class="vf-grid | vf-grid__col-2 | search-bar">
  		    <div class="bus-box">
  		    	<h5>Next bus in:&nbsp;<span class="badge badge-secondary">Live</span></h5>
            <div class="bus-number">Shuttle:<span class="bus-time">5 min</span></div>
            <div class="bus-number">39:<span class="bus-time">15 min</span></div>
            <div class="bus-number">39A:<span class="bus-time">9 min</span></div>
  		    </div>
          <div class="weather">
               <a class="weatherwidget-io" href="https://forecast7.com/en/49d408d67/heidelberg/" data-label_1="HEIDELBERG" data-label_2="WEATHER" data-font="Arial" data-icons="Climacons Animated" data-days="3" data-theme="pure" >HEIDELBERG WEATHER</a>
              <script>
              !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src='https://weatherwidget.io/js/widget.min.js';fjs.parentNode.insertBefore(js,fjs);}}(document,'script','weatherwidget-io-js');
              </script>
            </div>
  		  </div>
<!-- MAIN BAR -->
  <div class="vf-grid | vf-grid__col-2 | main-bar">
            <div class="news-container">
            	<h4 class="vf-section-header__heading">INsites</h4>
              <div class="vf-grid news-box">
{% for item in news.items %}
  <div id="news-image">
<img src="https://intranet.embl.de/communication_outreach/internal_news/2019/{{ item.image }}" class="image-ov">
  </div>
  <div id="news-text" >
<a href="{{ item.url }}"><h5>{{ item.title }}</h5></a>
<p>{{item.description}}</p>
<a href="#" class="vf-link vf-link--hover">Read all news >></a>
  </div>
{% endfor %}
</div>
            </div>
            <div class="col">
            	<h4 class="vf-section-header__heading">Services</h4>
              <div class="search">
             </div>
              <div class="services-icons">
              	<button class="button vf-button vf-button--tertiary"><i class="fas fa-check"></i> Room booking</button>
              	<button class="button vf-button vf-button--tertiary"><i class="si fa fa-download"></i> Downloads</button>
              	<button class="button vf-button vf-button--tertiary"><i class="fas fa-utensils"></i> Canteen Menu</button>
              	<button class="button vf-button vf-button--tertiary"><i class="si fa fa-bus"></i> Bus Shuttle</button>
              	<button class="button vf-button vf-button--tertiary"><i class="fas fa-chart-line"></i> Training</button>
              	<button class="button vf-button vf-button--tertiary"><i class="fas fa-book"></i> Library</button>
              	<button class="button vf-button vf-button--tertiary"><i class="far fa-file-alt"></i> Regulations</button>
              	<button class="button vf-button vf-button--tertiary"><i class="fas fa-euro-sign"></i> Salary</button>
              </div>
            </div>
      </div>
<!--SECONDARY BAR -->
<div class="vf-grid | vf-grid__col-3 | secondary-bar">
      <div class="this-week">
        <h4 class="vf-section-header__heading">Upcoming Events</h4>
           <div data-tockify-component="mini" data-tockify-calendar="emblcalendar"></div>
           <script data-cfasync="false" data-tockify-script="embed"
                                             src="https://public.tockify.com/browser/embed.js"></script>
      </div>
      <div class="seminars-list">
      <div class="seminars vf-text">
        <h4 class="vf-section-header__heading">Seminars</h4>
<div id="seminars-embed">
{%for item in seminars.items%}
<p class="seminar-date">{{item.date}}, {{item.room}}</p>
<h6 class="seminar-title">{{item.title}}</h6>
<p class="seminar-speaker">{{item.firstname}} {{item.name}}</p>
{%endfor%}
</div>
           <a href="#" class="vf-link vf-link--hover">Seminars calendar >></a>
      </div>
      <div class="carousel">
          <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
      <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img class="d-block w-100" src="../images/TBP19-01_290x144.jpg" alt="First slide">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="../images/TRC19-01_290x144.jpg" alt="Second slide">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="../images/VZB19-01_290x144.jpg" alt="Third slide">
      </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
        </div>
        <a href="#" class="vf-link vf-link--hover">Courses and Conferences >></a>
    </div>
   </div>
   <!--POPULAR LINKS -->
{% include "links.njk" %}
        <!-- SOCIAL MEDIA -->
      <div class="vf-grid | vf-grid__col-3 | social-media">
         <div class="sm-feeds">
          <!-- <i class="social-m fab fa-twitter-square"></i> -->
           <a class="twitter-timeline" data-width="370" data-height="400" href="https://twitter.com/embl?ref_src=twsrc%5Etfw">Tweets by embl</a> <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
         </div>
   </div>
</div>

