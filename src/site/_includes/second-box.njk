  <div class="vf-grid | vf-grid__col-3 | secondary-bar">
    <div class="this-week">
      <h4 class="vf-section-header__heading">Today at EMBL</h4>
      <div data-tockify-component="calendar" data-tockify-calendar="emblcalendar"></div>
      <script data-cfasync="false" data-tockify-script="embed" src="https://public.tockify.com/browser/embed.js">
      </script>
    </div>
    <div class="seminars">
      <h4 class="vf-section-header__heading">Seminars</h4>
      <div id="seminars-embed">
        {%for item in seminars.items%}
        <p class="vf-summary__date">{{item.date}} - {{item.room}}</p>
        <p class="vf-summary__date"></p>
        <h6 class="vf-summary">{{item.title}}</h6>
        <p class="vf-summary__date">{{item.firstname}} {{item.name}}</p>
        <hr>
        {%endfor%}
      </div>
      <div class="more-link">
        <a href="#" class="vf-link vf-link--hover">Seminars calendar >></a>
      </div>
      <div class="carousel">
        <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
          <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
          </ol>
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img class="d-block w-100" src="../images/TBP19-01_290x144.jpg" alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="../images/TRC19-01_290x144.jpg" alt="Second slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="../images/VZB19-01_290x144.jpg" alt="Third slide">
            </div>
          </div>
          <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>
      <div class="more-link">
        <a href="#" class="vf-link vf-link--hover | see-more">Courses and Conferences >></a>
      </div>
    </div>
    <div class="events">
      <h4 class="vf-section-header__heading">Internal Events</h4>
      <div>
      <img class="event-thumbnail" src="#">
      <p class="vf-summary__date">Heidelberg</p>
      <p class="vf-summary__date">Thursday 12 September 2019</p>
      <a href="#" class="vf-summary">Open Targets Workshop</a>
      </div>
      <div>
      <img class="event-thumbnail" src="#">
      <p class="vf-summary__date">Stromberg</p>
      <p class="vf-summary__date">Tuesday 17 September - Friday 20 September 2019</p>
      <a href="#" class="vf-summary">SSMAC / Faculty Retreat 2019</a>
      </div>
      <div>
      <img class="event-thumbnail" src="#">
      <p class="vf-summary__date">Heidelberg</p>
      <p class="vf-summary__date">Thursday 26 September 2019</p>
      <a href="#" class="vf-summary">EMBL–GSK Science Day 2019</a>
      </div>
      <div>
      <img class="event-thumbnail" src="#">
      <p class="vf-summary__date">Heidelberg</p>
      <p class="vf-summary__date">Friday 8 November 2019</p>
      <a href="#" class="vf-summary">EMBL Aid’s Mont Blanc Challenge</a>
      </div>
      <div class="more-link">
        <a href="#" class="vf-link vf-link--hover | see-more">Events calendar >></a>
      </div>
    </div>

  </div>
  <hr class="vf-divider">