<section class="vf-grid | vf-grid__col-2 | main-bar">
  <div class="news-container">
    <h4 class="vf-section-header__heading">INsites</h4>
    <div class="insites">
      {% for item in news.items %}
      <hr class="vf-divider">
      <div class="news-image">
        <img src="https://intranet.embl.de/communication_outreach/internal_news/2019/{{ item.image }}">
      </div>
      <div class="news-text">
        <p class="vf-summary__date">{{item.date}}</p>
        <a href="{{ item.url }}" class="vf-summary__link">
          <h5 class="vf-summary__title">{{ item.title }}</h5>
        </a>
        <p class="crop vf-summary__text">{{item.description}}</p>
        </br>
      </div>
      {% endfor %}
      <div class="more-link">
        <a href="#" class="vf-link vf-link--hover | see-more">Read all news >></a>
      </div>
    </div>
  </div>
  <div class="services-container">
      <div id="search-people">
        <form class="search" method="get" action="http://intranet.embl.de/search_result/index.php">
          <input type="text" placeholder="Search" name="search">
          <button type="submit"><i class="fa fa-search"></i></button>
        </form>
      </div>
      <div class="quick-links">
        <a class="test-i" href="#">
          <i class="fas fa-check icon-test"></i>
          <p class="icon-p">Room Booking</p>
        </a>
        <a class="test-i" href="#">
          <i class="fas fa-utensils icon-test"></i>
